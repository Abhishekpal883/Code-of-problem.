import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner read = new Scanner (System.in);
		int number_of_player = read.nextInt();
		int eliminate_player = read.nextInt();
		System.out.println((number_of_player - eliminate_player) * 10000);

	}
}
